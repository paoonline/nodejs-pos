<div class="panel" style="margin: 20px">
    <div class="heading">
        <span class="icon mif-home"></span>
        <span class="title">ข้อมูลร้าน</span>
    </div>
    <div class="content" style="padding: 20px">
        <form method="post">
            <div class="grid">
                <div class="row cells3">
                    <div class="cell">
                        <label>ชื่อ</label>
                        <div class="input-control full-size">
                            <input type="text" name="name" value="<%= org.name %>" />
                        </div>
                    </div>

                    <div class="cell">
                        <label>เบอร์โทร</label>
                        <div class="input-control full-size">
                            <input type="text" name="tel" value="<%= org.tel %>" />
                        </div>
                    </div>

                    <div class="cell">
                        <label>เลขผู้เสียภาษี</label>
                        <div class="input-control full-size">
                            <input type="text" name="tax" value="<%= org.tax %>" />
                        </div>
                    </div>
                    <div class="row cells1">
                        <label>ที่อยู่</label>
                        <div class="input-control text full-size">
                            <input type="text" name="address" value="<%= org.address %>" />
                        </div>
                    </div>
                </div>
                <div class="row cells1">
                    <div class="cell align-center">
                        <button class="button large-button bg-green fg-white">
                            <i class="mif-checkmark"></i>
                                บันทึก
                            
                        </button>
                    </div>
                </div>
            </div>
        </form>
    </div>
</div>